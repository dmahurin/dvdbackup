#. TRANSLATORS: --help output 1 (synopsis)
#: src/main.c:69
#, c-format
msgid "Usage: %s [OPTION]...\n"
msgstr "Uso: %s [OPZIONE]...\n"

#. TRANSLATORS: --help output 3: options 1/2
#. no-wrap
#: src/main.c:79
#, c-format
msgid ""
"  -h, --help         display this help and exit\n"
"  -V, --version      display version information and exit\n"
"\n"
msgstr ""
"  -h. --help mostra l'aiuto ed esce\n"
"-V, --version mostra le informazioni di versione ed esce\n"
"\n"

#. TRANSLATORS: --help output 4: options 2/2
#. no-wrap
#: src/main.c:85
#, c-format
msgid ""
"  -I, --info         prints information about the DVD\n"
"  -M, --mirror       backup the whole DVD\n"
"  -F, --feature      backup the main feature of the DVD\n"
"  -T, --titleset=X   backup title set X\n"
"  -t, --title=X      backup title X\n"
"  -s, --start=X      backup from chapter X\n"
"  -e, --end=X        backup to chapter X\n"
"\n"
msgstr ""
"  -I, --info stampa informazioni sul DVD\n"
"-M, --mirror  backup dell'intero DVD\n"
"-F, --feature backup dell'intera caratteristica del DVD\n"
"-T, --titleset=X impostare backup titolo X\n"
"-t, --title=X backup titolo X\n"
"-s, --start=X backup dal capitolo X\n"
"-e, --end=X backup fino al capitolo X\n"
"\n"

#: src/main.c:94
#, c-format
msgid ""
"  -i, --input=DEVICE       where DEVICE is your DVD device\n"
"                           if not given /dev/dvd is used\n"
"  -o, --output=DIRECTORY   where directory is your backup target\n"
"                           if not given the current directory is used\n"
msgstr ""
"  -i, --input=DISPOSITIVO dove DISPOSITIVO è il tuo drive DVD\n"
"                           se non viene passato nessun valore, viene usato "
"/dev/dvd\n"
"  -o, --output=CARTELLA dove CARTELLA è la destinazione del backup\n"
"                           se non viene passato nessun valore, viene usata "
"la cartella corrente\n"

#: src/main.c:99
#, c-format
msgid ""
"  -v, --verbose            print more information about progress\n"
"  -n, --name=NAME          set the title (useful if autodetection fails)\n"
"  -a, --aspect=0           to get aspect ratio 4:3 instead of 16:9 if both "
"are\n"
"                           present\n"
"  -r, --error={a,b,m}      select read error handling: a=abort, b=skip "
"block,\n"
"                           m=skip multiple blocks (default)\n"
"  -p, --progress           print progress information while copying VOBs\n"
"\n"
msgstr ""

#: src/main.c:108
#, c-format
msgid ""
"  -a is option to the -F switch and has no effect on other options\n"
"  -s and -e should preferably be used together with -t\n"
msgstr ""
"  -a è una opzione allo switch -F e non ha effetti sulle altre opzioni\n"
"  -s e -e dovrebbero essere usate preferibilmente assieme a -t\n"

